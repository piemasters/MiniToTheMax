    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "axe-core": "^4.1.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-remove-graphql-queries": "^3.2.0",
    "babel-preset-gatsby": "^1.2.0",
    "babel-preset-react-app": "^10.0.0",
    "csstype": "^3.0.7",
    "cypress": "^6.8.0",
    "cypress-axe": "^0.12.2",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
