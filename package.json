    "babel-preset-gatsby": "^0.8.0",
    "babel-preset-react-app": "^10.0.0",
    "csstype": "^3.0.5",
    "cypress": "^6.1.0",
    "cypress-axe": "^0.12.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
