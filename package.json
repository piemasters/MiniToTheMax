{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-dev": "env-cmd gatsby build",
    "develop": "env-cmd  gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "storybook": "set NODE_ENV=production&& start-storybook -p 6006 -s static",
    "build-storybook": "set NODE_ENV=production&& build-storybook -s static"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.36",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-google-gtag": "^1.1.19",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-mdx": "^1.0.62",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "^2.3.7",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-images": "^3.1.38",
    "gatsby-remark-images-modal": "0.0.4",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "gsap": "^3.0.4",
    "lodash": "latest",
    "lodash.kebabcase": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-grid-gallery": "^0.5.5",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.17",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-remove-graphql-queries": "^2.7.19",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-graphql": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "typescript": "^3.7.3"
