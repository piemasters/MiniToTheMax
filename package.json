{
  "name": "minitothemax",
  "private": true,
  "description": "Website for MiniToTheMax",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn preoptimize&& gatsby build",
    "build-dev": "env-cmd gatsby build",
    "develop": "env-cmd  gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,tsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:output": "jest --json --outputFile=.jest-results.json || true",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "prestorybook": "yarn test:output",
    "storybook": "set NODE_ENV=production&& start-storybook -p 6006 -s public,content/assets",
    "build-storybook": "set NODE_ENV=production&& build-storybook -s public,content/assets",
    "preoptimize": "node ./src/scripts/optimize.js"
  },
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "6.2.1",
