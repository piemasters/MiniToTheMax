    "gatsby": "^2.28.2",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-catch-links": "^2.6.0",
    "gatsby-plugin-emotion": "^5.0.0",
    "gatsby-plugin-google-gtag": "^2.5.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-mdx": "^1.6.0",
    "gatsby-plugin-offline": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-typescript": "^2.8.0",
    "gatsby-remark-images": "^3.7.1",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "gsap": "^3.5.1",
    "lodash.kebabcase": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-gallery": "^0.5.5",
    "react-helmet": "^6.1.0"
  },
