{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-dev": "env-cmd gatsby build",
    "develop": "env-cmd  gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,tsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:output": "jest --json --outputFile=.jest-results.json || true",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "prestorybook": "yarn test:output",
    "storybook": "set NODE_ENV=production&& start-storybook -p 6006 -s public",
    "build-storybook": "set NODE_ENV=production&& build-storybook -s public"
  },
  "dependencies": {
