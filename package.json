    "disqus-react": "^1.0.11",
    "gatsby": "^3.2.1",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-catch-links": "^3.2.0",
    "gatsby-plugin-emotion": "^6.2.0",
    "gatsby-plugin-google-gtag": "^3.2.0",
    "gatsby-plugin-image": "^1.2.1",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-mdx": "^2.2.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^4.2.0",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-plugin-sitemap": "^3.2.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-typescript": "^3.2.0",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "gsap": "^3.6.1",
    "lodash.kebabcase": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/jest": "^11.2.1",
    "@storybook/addon-a11y": "^6.2.2",
    "@storybook/addon-essentials": "^6.2.2",
    "@storybook/addon-jest": "^6.2.2",
    "@storybook/addon-links": "^6.2.2",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/react": "^6.2.2",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/node": "^14.14.37",
    "@types/node-sass": "^4.11.1",
